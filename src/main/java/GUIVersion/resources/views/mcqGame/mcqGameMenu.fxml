<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.mcqGameControllers.MCQGameMenuController">
  <children>
      <ImageView fitHeight="640.0" fitWidth="900.0" pickOnBounds="true">
         <image>
            <Image url="@../../images/mcqBackMenu.png" />
         </image>
      </ImageView>
    <Button fx:id="quitButton" layoutX="641.0" layoutY="531.0" mnemonicParsing="false" onAction="#changeSceneToGameCenter" prefHeight="43.0" prefWidth="176.0" style="-fx-background-radius: 10;" text="QUIT">
      <font>
        <Font size="20.0" />
      </font>
    </Button>
    <Button fx:id="highScoreButton" layoutX="103.0" layoutY="532.0" mnemonicParsing="false" onAction="#changeSceneToHighScore" prefHeight="41.0" prefWidth="176.0" style="-fx-background-radius: 10;" text="HIGH SCORE">
      <font>
        <Font size="21.0" />
      </font>
    </Button>
    <Button fx:id="playButton" layoutX="371.0" layoutY="532.0" mnemonicParsing="false" onAction="#changeSceneToGamePlay" prefHeight="41.0" prefWidth="175.0" style="-fx-background-radius: 10;" text=" PLAY">
      <font>
        <Font size="21.0" />
      </font>
    </Button>
  </children>
</AnchorPane>
